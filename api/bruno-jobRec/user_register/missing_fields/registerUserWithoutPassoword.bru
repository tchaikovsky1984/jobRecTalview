meta {
  name: registerUserWithoutPassoword
  type: http
  seq: 1
}

post {
  url: {{baseURL}}user/register
  body: json
  auth: inherit
}

body:json {
  {
    "username": "acdska",
    "email": "aja@janfja.com",
    "name": "ACD"
  }
}

assert {
  res.status: eq 400
  res.body.error: eq "password not provided"
  req.url: eq http://localhost:7171/user/register
}

settings {
  encodeUrl: true
  timeout: 0
}
