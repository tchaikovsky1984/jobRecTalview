meta {
  name: registerUserWithNoUsername
  type: http
  seq: 2
}

post {
  url: {{baseURL}}user/register
  body: json
  auth: inherit
}

body:json {
  {
    "username": "",
    "email": "aja@janfja.com",
    "name": "ACD",
